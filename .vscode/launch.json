    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/NewCleanArchProject/bin/Debug/net7.0/NewCleanArchProject.dll",
            "args": [
                "-np",
                "/Users/eduardo/Projects/FRG/Interoperabilidade",
                "AutoEmailSender",
                "-es",
                "Email",
                "-ui",
                "console",
                "-db",
                "none",
                "-repo"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}